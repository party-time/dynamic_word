.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /*padding: 200rpx 0;*/
  box-sizing: border-box;
}

.box {
  width: 100%;
  position: relative;
}

.main {
  width: 95%;
  height: 50px;
  background: #eee;
  border: 1px solid #ccc;
  margin: 5px auto;
  text-align: center;
  line-height: 50px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-right: 10rpx;
  font-size: 10pt;
}

.tttttttttttttt{
  width: 70%;
  text-align: left;
  padding-left: 10rpx;
}

.mainmove {
  position: absolute;
  opacity: 0.7;
}

.maind {
  background: #fff;
  border: 1px dashed #efefef;
}

.mainend {
  position: static;
  opacity: 1;
}
